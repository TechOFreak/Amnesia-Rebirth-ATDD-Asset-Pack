  <Subject Desc="Caves to Cistern Fall." Name="Global_BabyTalk_CavesToCisternStart" UseSingleRandomLine="True" SceneId="8" EffectId="-1" Section="Generic Baby Talk">
   <Line CharacterId="2" Prio="0" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="A sister for Alys... or a brother, maybe. But only if I can get home." Timestamp="2018-01-26 02:15" />
   </Line>
   <Line CharacterId="2" Prio="-1" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Everything we wanted... why now?" Timestamp="2019-03-13 04:40" />
   </Line>
   <Line CharacterId="2" Prio="-2" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="There's nothing I can do about it. I just need to be careful." Timestamp="2019-03-13 04:40" />
   </Line>
   <Line CharacterId="2" Prio="-3" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I'll get through this, Salim." Timestamp="2019-03-13 04:40" />
   </Line>
   <Line CharacterId="2" Prio="-4" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I shouldn't have taken this risk." Timestamp="2019-03-13 04:40" />
   </Line>
   <Line CharacterId="2" Direction="*sigh*" Prio="-5" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Salim, I wish you were with me." Timestamp="2019-03-13 04:40" />
   </Line>
   <Line CharacterId="2" Prio="-6" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I'll try to get through this, for you." Timestamp="2019-03-13 04:40" />
   </Line>
   <Line CharacterId="2" Prio="-7" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="We should have turned down the job as soon as I realized." Timestamp="2019-07-31 12:00" />
   </Line>
   <Line CharacterId="2" Prio="-8" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Salim, how can I do this without you?" Timestamp="2019-03-13 04:40" />
