  <Subject Desc="In Alchemists." Name="Global_BabyTalk_Alchemists" UseSingleRandomLine="True" SceneId="8" EffectId="-1" Section="Generic Baby Talk">
   <Line CharacterId="2" Prio="0" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I'm so glad you're all right." Timestamp="2019-07-31 04:00" />
   </Line>
   <Line CharacterId="2" Prio="-1" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I have no idea where we are. I'm glad you're here, though." Timestamp="2020-02-27 05:08" />
   </Line>
   <Line CharacterId="2" Prio="-3" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Look at this place. It isn't Arabic, or Tuareg, or Ottoman, or anything I recognise." Timestamp="2019-07-31 04:00" />
   </Line>
   <Line CharacterId="2" Prio="-4" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I keep wondering if I died in that crash..." EndPadding="-0.25" Timestamp="2019-07-31 04:00" />
    <Sound Text="But this can't be hell, because you're still here. And it sure as hell isn't heaven." Timestamp="2019-07-31 04:00" />
   </Line>
   <Line CharacterId="2" Prio="-5" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Everything smells so old here. Like rust and wet copper." Timestamp="2019-07-31 04:00" />
   </Line>
   <Line CharacterId="2" Prio="-6" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I know this place is strange. But I promise I'll get you home." EndPadding="-0.5" Timestamp="2019-07-31 04:00" />
    <Sound Text="You'll love it, little one. Sunset over the Seine. The Jardin in autumn - oh, the colours of the leaves!" EndPadding="-0.5" Timestamp="2019-07-31 04:00" />
    <Sound Text="I have so much to show you." Timestamp="2019-07-31 04:00" />
   </Line>
   <Line CharacterId="2" Prio="-7" TextStatus="ToTranslators" AudioStatus="Final">
    <Sound Text="*hums the lullaby*" Timestamp="2019-07-31 04:00" />
   </Line>
