  <Subject Desc="Oasis to Alchemists - Tasi thinks baby is hurt/quiet/dead." Name="Global_BabyTalk_OasisToAlchemists" UseSingleRandomLine="True" SceneId="8" EffectId="-1" Section="Generic Baby Talk">
   <Line CharacterId="2" Prio="0" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I'm sorry, my love. Just... just let me know you're all right?" Timestamp="2019-08-02 08:51" />
   </Line>
   <Line CharacterId="2" Prio="-1" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Are you there, little one?" Timestamp="2019-08-02 08:51" />
   </Line>
   <Line CharacterId="2" Prio="-2" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="You're okay, aren't you... just... sleeping." Timestamp="2019-07-31 04:00" />
   </Line>
   <Line CharacterId="2" Prio="-3" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Please be there. Please." Timestamp="2019-07-31 04:00" />
