  <Subject Desc="In the Cistern, post Baby Kick." Name="Global_BabyTalk_CisternStartToOasis" UseSingleRandomLine="True" SceneId="8" EffectId="-1" Section="Generic Baby Talk">
   <Line CharacterId="2" Prio="0" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Hello, hello, little one. It's nice to know you." FileName="babytalk_global_babytalk_cisternstarttooasis_001_player_001" Timestamp="2019-01-15 03:52" />
   </Line>
   <Line CharacterId="2" Prio="-1" TextStatus="Recordable" PlayOnce="True">
    <Sound Text="I don't know what's happening to me, but if you're here, it can't be too bad." FileName="babytalk_global_babytalk_cisternstarttooasis_012_player_001" Timestamp="2020-08-04 01:47" />
   </Line>
   <Line CharacterId="2" Prio="-2" TextStatus="Recordable" PlayOnce="True">
    <Sound Text="We'll get through this. I just need to stay out of the darkness." FileName="babytalk_global_babytalk_cisternstarttooasis_013_player_001" Timestamp="2020-08-04 01:47" />
   </Line>
   <Line CharacterId="2" Prio="-3" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I'm sorry we're in this horrible place. We'll get out, I swear." FileName="babytalk_global_babytalk_cisternstarttooasis_002_player_001" Timestamp="2019-01-15 03:52" />
   </Line>
   <Line CharacterId="2" Prio="-4" TextStatus="Recordable" PlayOnce="True">
    <Sound Text="I'll be okay, my love, just need to keep calm, to remember what the doctor said." FileName="babytalk_global_babytalk_cisternstarttooasis_011_player_001" Timestamp="2020-08-04 01:47" />
   </Line>
   <Line CharacterId="2" Prio="-5" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="We're going to get out of this, you and I. For your papa, and for your sister." FileName="babytalk_global_babytalk_cisternstarttooasis_003_player_001" Timestamp="2019-01-15 03:52" />
   </Line>
   <Line CharacterId="2" Prio="-6" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I know, I know it's horrible. But we're together." FileName="babytalk_global_babytalk_cisternstarttooasis_004_player_001" Timestamp="2019-01-15 03:52" />
   </Line>
   <Line CharacterId="2" Prio="-7" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="Hey, little one. Don't worry. We'll be okay." FileName="babytalk_global_babytalk_cisternstarttooasis_005_player_001" Timestamp="2019-01-15 03:52" />
   </Line>
   <Line CharacterId="2" Prio="-8" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="I wish you'd met your papa." EndPadding="-0.25" FileName="babytalk_global_babytalk_cisternstarttooasis_006_player_001" Timestamp="2019-03-13 04:40" />
    <Sound Text="I'll... I'll have to show you pictures." EndPadding="-0.25" FileName="babytalk_global_babytalk_cisternstarttooasis_006_player_002" Timestamp="2019-03-13 04:40" />
    <Sound Text="He's still with us, all the time, now. Don't worry." FileName="babytalk_global_babytalk_cisternstarttooasis_006_player_003" Timestamp="2019-03-13 04:40" />
   </Line>
   <Line CharacterId="2" Prio="-9" TextStatus="ToTranslators" AudioStatus="Final" PlayOnce="True">
    <Sound Text="*hums the lullaby*" FileName="babytalk_global_babytalk_cisternstarttooasis_007_player_001" Timestamp="2019-03-13 04:40" />
   </Line>
   <Line CharacterId="2" Prio="-10" TextStatus="ToTranslators" AudioStatus="Final">
    <Sound Text="*sigh*" FileName="babytalk_global_babytalk_cisternstarttooasis_008_player_001" Timestamp="2019-10-14 02:30" />
