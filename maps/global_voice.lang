        <Entry Name="BabyTalk_Global_BabyTalk_Carried_004_Player_001">Hush, hush, my love. Hush, hush.</Entry>
        <Entry Name="BabyTalk_Global_BabyTalk_Carried_005_Player_001">*hums the lullaby*</Entry>
        <Entry Name="BabyTalk_Global_BabyTalk_Carried_006_Player_001">*hums the lullaby*</Entry>
        <Entry Name="BabyTalk_Global_BabyTalk_Carried_007_Player_001">*sigh*</Entry>
